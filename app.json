{
  "expo": {
    "name": "News",
    "slug": "News",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/news.jpg",
    "splash": {
      "image": "./src/assets/images/splashnews.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splashnews.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.norovnurshod.news"
    },
    "web": {
      "favicon": "./assets/images/splashnews.jpg"
    }
  }
}
